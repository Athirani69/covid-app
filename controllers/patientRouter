const express=require("express")
const router=express.Router()
const patModel=require("../models/patientModel")


router.post("/patiententry",async(req,res)=>{
   let data=req.body
   let patobj=new patModel(data)
   let result=await patobj.save()
   res.json({
    status:"success"
   })
})

router.get("/viewall",async(req,res)=>{
    let data=await patModel.find()
    res.json(data)
})

module.exports=router;